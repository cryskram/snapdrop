generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Note {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  slug      String   @unique
  content   String
  updatedAt DateTime @updatedAt
}
